<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bit.chinjs1.welcometodunedin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitiesActivity" android:parentActivityName=".MainActivity"/>
        <activity android:name=".shoppingActivity" android:parentActivityName=".MainActivity" />
        <activity android:name=".diningActivity" android:parentActivityName=".MainActivity" />
        <activity android:name=".servicesActivity" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>